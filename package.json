{
  "name": "k6-electron-gui",
  "version": "0.1.0",
  "private": true,
  "description": "Minimal Electron GUI to run TypeScript k6 GET test (10 VUs example)",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:k6": "esbuild src/k6/test.ts --bundle --platform=node --target=node16 --outfile=dist/test.bundle.js --external:k6",
    "build": "npm run build:k6",
    "prepare": "npm run build",
    "lint": "echo 'no linter configured'"
  },
  "devDependencies": {
    "esbuild": "^0.19.0",
    "electron": "^26.0.0",
    "typescript": "^5.6.0",
    "cpy-cli": "^4.0.1"
  },
  "files": ["dist/**", "main.js", "package.json"]
}
